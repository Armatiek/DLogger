<?xml version="1.0"?>
<webapp 
  xmlns="http://www.armatiek.com/xslweb/webapp"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.armatiek.com/xslweb/webapp ../../config/xsd/xslweb/webapp.xsd">
  
  <title>DLogger</title>
  <description>DLogger</description>
  
  <development-mode>false</development-mode>
  <max-upload-size>100</max-upload-size><!-- 100MB max upload-->
  <wait-for-jobs-at-close>true</wait-for-jobs-at-close>
  
  <!-- Resources to serve straight away: -->
  <resources>
    <resource pattern="/.+\.css" media-type="text/css"/>
    <resource pattern="/.+\.js" media-type="text/javascript"/>
    <resource pattern="/.+\.css\.map" media-type="text/plain"/>
    <resource pattern="/.+\.js\.map" media-type="text/javascript"/>
    <resource pattern="/.+/release\.xml" media-type="application/xml"/>
  </resources>

  <!-- Stylesheet parameters: -->
  <parameters/>
  
  <!-- Scheduled job definitions: -->
   <jobs/>
  

</webapp>